#!/usr/bin/env python3
from yet_another_wizz.infrastructure.parser import parser
from yet_another_wizz.infrastructure import jobs


def main():
    args = parser.parse_args() 
    if args.job is None:
        parser.print_usage()
    else:  # run
        job = getattr(jobs, args.job)
        job(args)


if __name__ == "__main__":
    main()
