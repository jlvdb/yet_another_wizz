#!/usr/bin/env python3
from yaw.pipe.parser import parser
from yaw.pipe import jobs


def main():
    args = parser.parse_args()
    if args.job is None:
        parser.print_usage()
    else:  # run
        job = getattr(jobs, args.job)
        job(parser, args)


if __name__ == "__main__":
    main()
